{
  "name": "cucumber_cypress",
  "version": "1.0.0",
  "description": "cypress-cucumber",
  "main": "index.js",
  "scripts": {
    "cypress:run": "cypress run --browser chrome --config-file conf/cypress.config.js",
    "test": "cypress open",
    "go": "npx cypress run --config-file conf/my.config.js --browser chrome && node ./cucumber-html-report.js",
    "gohead": "npx cypress run --config-file conf/my.config.js --browser chrome --headed && node ./cucumber-html-report.js",
    "runtest": "cypress run --browser chrome --config-file conf/cypress.config.js",
    "runrep": "node cucumber-html-report.js"

  },
  "keywords": [
    "cucumber",
    "cypress"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@badeball/cypress-cucumber-preprocessor": "^13.0.2",
    "@bahmutov/cypress-esbuild-preprocessor": "^2.1.5",
    "cypress": "^10.9.0",
    "esbuild": "^0.15.9",
    "multiple-cucumber-html-reporter": "^3.0.1",
    "rimraf": "^3.0.2"
  }
}
